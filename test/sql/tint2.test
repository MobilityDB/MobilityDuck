require mobilityduck

query I
SELECT '15@2025-01-01'::TINT2 as tint;
----
15@2025-01-01 00:00:00+00

query I
SELECT '100@2025-01-01 15:00:00'::TINT2 as tint;
----
100@2025-01-01 15:00:00+00

query I
SELECT '100@2025-01-01 10:00:00+05'::TINT2 as tint;
----
100@2025-01-01 05:00:00+00

query I
SELECT '{1@2025-01-01, 2@2025-01-02, 1@2025-01-03}'::TINT2 as tint;
----
{1@2025-01-01 00:00:00+00, 2@2025-01-02 00:00:00+00, 1@2025-01-03 00:00:00+00}

query I
SELECT '{[1@2025-01-01, 2@2025-01-02],[3@2025-01-04, 3@2025-01-05]}'::TINT2 as tint;
----
{[1@2025-01-01 00:00:00+00, 2@2025-01-02 00:00:00+00], [3@2025-01-04 00:00:00+00, 3@2025-01-05 00:00:00+00]}

query I
SELECT TINT2(42, '2023-01-01'::TIMESTAMPTZ) as tint;
----
42@2023-01-01 00:00:00+00

query I
SELECT TINT2(42, '2023-01-01 12:00:00'::TIMESTAMPTZ) as tint;
----
42@2023-01-01 12:00:00+00

query I
SELECT TINT2(42, '2023-01-01 12:00:00+05'::TIMESTAMPTZ) as tint;
----
42@2023-01-01 07:00:00+00

query I
SELECT tempSubtype('1@2025-01-01'::TINT2) as tempType;
----
Instant

query I
SELECT tempSubtype('{1@2025-01-01, 2@2025-01-02, 1@2025-01-03}'::TINT2) as tempType;
----
Sequence

query I
SELECT tempSubtype('{[1@2025-01-01, 2@2025-01-02, 1@2025-01-03],[3@2025-01-04, 3@2025-01-05]}'::TINT2) as tempType;
----
SequenceSet

query I
SELECT getValue(TINT2(42, '2023-01-01 12:00:00'::TIMESTAMPTZ)) as value;
----
42

query I
SELECT getTimestamp('1@2000-01-01'::TINT2) as ts;
----
2000-01-01 00:00:00+00

query I
SELECT sequences(tint2 '[1@2000-01-01, 2@2000-01-02, 1@2000-01-03]');
----
['[1@2000-01-01 00:00:00+00, 2@2000-01-02 00:00:00+00, 1@2000-01-03 00:00:00+00]']

query I
SELECT sequences(tint2 '{[1@2000-01-01], [3@2000-01-04, 3@2000-01-05]}');
----
['[1@2000-01-01 00:00:00+00]', '[3@2000-01-04 00:00:00+00, 3@2000-01-05 00:00:00+00]']